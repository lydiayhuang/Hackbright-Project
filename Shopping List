shopping_list = []

def listing(item):
	if item.lower() in shopping_list:
		print "List already contains", item
	else:
		shopping_list.append(item)
		shopping_list.sort()
		print shopping_list	
		return shopping_list
	

listing(raw_input("What do you want to add to the list?").lower())
listing(raw_input("What do you want to add to the list?").lower())
